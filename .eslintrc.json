{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "next/core-web-vitals"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/prop-types": "off",
    "no-multiple-empty-lines": "error",
    "no-extra-semi": "error",
    "no-trailing-spaces": "error",
    "no-dupe-keys": "error"
  }
}
